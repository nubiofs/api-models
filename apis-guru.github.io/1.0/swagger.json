      },
      "description": "List of API details.\nIt consist of JSON object with API IDs(`<provider>[:<service>]`) as keys.\n",
      "minProperties": 1,
      "type": "object"
    },
    "ApiVersion": {
      "additionalProperties": false,
      "properties": {
        "added": {
          "description": "Timestamp when this version was added",
          "format": "date-time",
          "type": "string"
        },
