          "type": "object"
        },
        "swaggerUrl": {
          "description": "URL of Swagger spec in JSON format",
          "format": "url",
          "type": "string"
        },
        "swaggerYamlUrl": {
          "description": "URL of Swagger spec in YAML format",
          "format": "url",
          "type": "string"
        },
        "updated": {
          "description": "Timestamp when this version was updated",
          "format": "date-time",
          "type": "string"
        }
      },
      "required": [
        "added",
        "updated",
        "swaggerUrl",
        "swaggerYamlUrl",
        "info"
      ],
      "type": "object"
    }
